<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.OrdiniClienti" table="OrdiniClienti">
        <id name="id" type="long" column="id">
            <generator class="native" />
        </id>
        <property name="idCliente" type="long" column="idcliente" />
        <property name="dataOrdine" type="string" column="dataordine" />
        <property name="costoTotale" type="double" column="costototale" />
        <property name="stato" type="string" column="stato"/>
    </class>

</hibernate-mapping>