<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Assistenza" table="Assistenza">
		<id name="id" type="long" column="id">
			<generator class="native"></generator>
		</id>
		<property name="idcliente" type="long" column="idcliente" />
		<property name="idordine" type="long" column="idordine" />
		<property name="descrizione" type="string" column="descrizione" />
	</class>
</hibernate-mapping>